<h1>Posts</h1>

<div class="container">
  <table class="table table-striped">
    <thead>
    <tr>
      <th>Title</th>
      <th>Content</th>
      <th>Actions</th>
    </tr>
    </thead>
    <tbody>
    <% @posts.each do |post| %>
      <tr>
        <td><%= post.title %></td>
        <td><%= post.content %></td>
        <td> <%= link_to 'Show', post %> </td>
      </tr>
    <% end %>
    </tbody>
  </table>
</div>
